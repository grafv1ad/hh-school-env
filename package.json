{
  "name": "hh-school-env",
  "author": "Vladislav Petrov",
  "homepage": "https://grafv1ad.github.io/hh-school-env/",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && stylelint src/*.{css,less} --max-warnings 0",
    "lint-fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix && stylelint src/*.{css,less} --max-warnings 0 --fix",
    "pretty": "prettier .",
    "pretty-check": "prettier . --check",
    "preview": "vite preview",
    "postinstall": "husky install",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@hh.ru/eslint-config": "^15.1.1",
    "@hh.ru/eslint-plugin-import-rules": "^1.5.2",
    "@hh.ru/stylelint-config-hh": "^10.2.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-dollar-sign": "^1.0.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "prettier": "^3.1.0",
    "stylelint": "^13.13.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "~3.19.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "packageManager": "yarn@4.0.2"
}
